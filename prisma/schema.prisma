generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model post {
  id        Int      @id @default(autoincrement())
  title     String
  image     String
  description String?
  creatdAt  DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model playing_with_neon {
  id    Int    @id @default(autoincrement())
  name  String
  value Float? @db.Real
}
